<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="35dp"
    tools:background="@color/ski_color_A86A91"
    android:gravity="center"
    android:paddingStart="16dp"
    android:paddingEnd="16dp">

    <com.ski.box.view.view.cus.CusTextView
        android:id="@+id/tv_current_period"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        tools:text="20160304-0945"
        android:textColor="@color/ski_color_333333"
        android:textSize="14dp"
        android:gravity="start|center_vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />


    <ImageView
        android:id="@+id/tv_num_1"
        style="@style/Top_result_lottery_style_pk10"
        tools:text="1"
        app:layout_constraintStart_toEndOf="@+id/tv_current_period"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tv_num_2"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginStart="10dp"/>

    <ImageView
        android:id="@+id/tv_num_2"
        style="@style/Top_result_lottery_style_pk10"
        tools:text="2"
        app:layout_constraintStart_toEndOf="@+id/tv_num_1"
        app:layout_constraintTop_toTopOf="@+id/tv_num_1"
        app:layout_constraintEnd_toStartOf="@+id/tv_num_3"/>

    <ImageView
        android:id="@+id/tv_num_3"
        style="@style/Top_result_lottery_style_pk10"
        tools:text="3"
        app:layout_constraintStart_toEndOf="@+id/tv_num_2"
        app:layout_constraintTop_toTopOf="@+id/tv_num_1"
        app:layout_constraintEnd_toStartOf="@+id/tv_num_4"/>

    <ImageView
        android:id="@+id/tv_num_4"
        style="@style/Top_result_lottery_style_pk10"
        tools:text="4"
        app:layout_constraintStart_toEndOf="@+id/tv_num_3"
        app:layout_constraintTop_toTopOf="@+id/tv_num_1"
        app:layout_constraintEnd_toStartOf="@+id/tv_num_5"/>

    <ImageView
        android:id="@+id/tv_num_5"
        style="@style/Top_result_lottery_style_pk10"
        tools:text="5"
        app:layout_constraintStart_toEndOf="@+id/tv_num_4"
        app:layout_constraintTop_toTopOf="@+id/tv_num_1"
        app:layout_constraintEnd_toStartOf="@+id/tv_num_6"/>

    <ImageView
        android:id="@+id/tv_num_6"
        style="@style/Top_result_lottery_style_pk10"
        tools:text="6"
        app:layout_constraintStart_toEndOf="@+id/tv_num_5"
        app:layout_constraintTop_toTopOf="@+id/tv_num_1"
        app:layout_constraintEnd_toStartOf="@+id/tv_num_7"/>

    <ImageView
        android:id="@+id/tv_num_7"
        style="@style/Top_result_lottery_style_pk10"
        tools:text="7"
        app:layout_constraintStart_toEndOf="@+id/tv_num_6"
        app:layout_constraintTop_toTopOf="@+id/tv_num_1"
        app:layout_constraintEnd_toStartOf="@+id/tv_num_8"/>

    <ImageView
        android:id="@+id/tv_num_8"
        style="@style/Top_result_lottery_style_pk10"
        tools:text="8"
        app:layout_constraintStart_toEndOf="@+id/tv_num_7"
        app:layout_constraintTop_toTopOf="@+id/tv_num_1"
        app:layout_constraintEnd_toStartOf="@+id/tv_num_9"/>

    <ImageView
        android:id="@+id/tv_num_9"
        style="@style/Top_result_lottery_style_pk10"
        tools:text="9"
        app:layout_constraintStart_toEndOf="@+id/tv_num_8"
        app:layout_constraintTop_toTopOf="@+id/tv_num_1"
        app:layout_constraintEnd_toStartOf="@+id/tv_num_10"/>

    <ImageView
        android:id="@+id/tv_num_10"
        style="@style/Top_result_lottery_style_pk10"
        tools:text="10"
        app:layout_constraintStart_toEndOf="@+id/tv_num_9"
        app:layout_constraintTop_toTopOf="@+id/tv_num_1"
        app:layout_constraintEnd_toStartOf="@+id/iv_arrow_down_result"
        android:layout_marginEnd="5dp"/>


    <ImageView
        android:id="@+id/iv_arrow_down_result"
        android:layout_width="14dp"
        android:layout_height="14dp"
        android:src="@mipmap/icon_bet_arrow_open_result"
        app:layout_constraintTop_toTopOf="@+id/tv_num_1"
        app:layout_constraintEnd_toEndOf="parent"
        android:visibility="invisible"/>
</androidx.constraintlayout.widget.ConstraintLayout>